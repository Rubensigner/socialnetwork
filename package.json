{
  "name": "socialnetwork",
  "version": "1.0.0",
  "description": "Start your own social network platform",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build && node index",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rubensigner/socialnetwork.git"
  },
  "author": "Rubens Mesquita",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Rubensigner/socialnetwork/issues"
  },
  "homepage": "https://github.com/Rubensigner/socialnetwork#readme",
  "dependencies": {
    "angular": "^1.5.8",
    "angular-route": "^1.5.8",
    "angular-sanitize": "^1.5.8",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "cors": "^2.7.1",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "jsconfig": "^0.2.0",
    "jsonwebtoken": "^7.1.7",
    "moment": "^2.14.1",
    "morgan": "^1.7.0",
    "mysql": "^2.11.1",
    "ng-infinite-scroll": "^1.3.0",
    "path": "^0.12.7",
    "promise": "^7.1.1",
    "socket.io": "^1.4.8",
    "twin-bcrypt": "^2.1.1",
    "validator": "^6.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-prometheusresearch": "^0.1.0",
    "babel-preset-stage-0": "^6.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^3.4.0",
    "nodemon": "^1.10.2"
  }
}
